<h1>Sign in</h1>
<%= form_for(:session, :url => sessions_path) do |f|%>

	<div class='field'>
		<%= f.label :emai %><br/>
		<%= f.text_field :email %>
	</div>
	<div class='field'>
		<%= f.label :password %><br/>
		<%= f.password_field :password %>
	</div>
	<div class='actions'>
		<%= f.submit "Sign Up" %>
	</div>
<% end %>

<p>New User <%= link_to "Sign up now!", signup_path%></p>